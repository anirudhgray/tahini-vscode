{
  "name": "tahini",
  "publisher": "anirudhgray",
  "icon": "./icons/tahini-file-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/anirudhgray/tahini-vscode.git"
  },
  "displayName": "tahini",
  "description": "Syntax highlighting for the Tahini programming language.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "tahini",
        "aliases": [
          "Tahini",
          "tahini"
        ],
        "extensions": [
          ".tah"
        ],
        "icon": {
          "light": "./icons/tahini-file-icon.svg",
          "dark": "./icons/tahini-file-icon.svg"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tahini",
        "scopeName": "source.tahini",
        "path": "./syntaxes/tahini.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "tahini",
        "path": "./snippets/tahini.snippets.json"
      }
    ]
  }
}
